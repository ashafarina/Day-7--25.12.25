public class ExamResult {
    public static void main(String[] args) {

        int[] marks = {65, 70, 58};
        int total = 0;

        for (int m : marks)
            total += m;

        double avg = total / 3.0;

        System.out.println("Average: " + avg);
        System.out.println(avg >= 50 ? "Pass" : "Fail");
    }
}
